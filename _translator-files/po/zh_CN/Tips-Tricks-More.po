msgid ""
"<?php\n"
"function loadserverstat ( $statfilename )\n"
"{\n"
"   $datei = fopen ( $statfilename, \"r\" );\n"
"   while ( !feof ( $datei ) )\n"
"   {\n"
"          $buffer = fgets ( $datei, 4096 );\n"
"          echo $buffer;\n"
"   }\n"
"   fclose($datei);\n"
"}\n"
"?>\n"
"<?php loadserverstat ( \"stat1.dat\" ); ?>\n"
msgstr ""
"~~~\n"
"<?php\n"
"function loadserverstat ( $statfilename )\n"
"{\n"
"   $datei = fopen ( $statfilename, \"r\" );\n"
"   while ( !feof ( $datei ) )\n"
"   {\n"
"          $buffer = fgets ( $datei, 4096 );\n"
"          echo $buffer;\n"
"   }\n"
"   fclose($datei);\n"
"}\n"
"?>\n"
"<?php loadserverstat ( \"stat1.dat\" ); ?>\n"
"~~~\n"

